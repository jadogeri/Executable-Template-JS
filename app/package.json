{
  "name": "executable-template-js",
  "version": "1.0.0",
  "description": "Node app (template) for generating executable files with javascript ",
  "type": "commonjs",
  "main": "script.js",
  "bin": "./script.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg ."
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "pkg": {
    "targets": [
      "node16",
      "node16-win-x64",
      "node16-linux-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist",
    "scripts": "src/**/*.js",
    "sourceType": "module"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "pkg": "^5.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jadogeri/Executable-Template-JS.git"
  },
  "keywords": [
    "executable",
    "mac",
    "linux",
    "windows",
    "pkg",
    "node",
    "javascript"
  ],
  "author": "Joseph Adogeri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jadogeri/Executable-Template-JS/issues"
  },
  "homepage": "https://github.com/jadogeri/Executable-Template-JS#readme"
}
